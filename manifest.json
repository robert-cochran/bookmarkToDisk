{

  "manifest_version": 2,
  "name": "bookmarkToDisk",
  "version": "1.0",

  "description": "Sends current url to nb-bookmarks server",

  "icons": {
    "48": "icons/border-48.png"
  },

  "content_scripts": [
    {
        "matches": ["<all_urls>"],
        "js": ["bookmarkToDisk.js"]
    }
  ]

}
